{
    "name": "totten/loco",
    "description": "Loco is the YAML-based \"local-compose\" process manager",
    "require": {
        "marcj/topsort": "^1.1",
        "symfony/yaml": "~3.0",
        "symfony/finder": "~3.0",
        "symfony/console": "~3.0",
        "symfony/filesystem": "~3.0",
        "symfony/event-dispatcher": "~3.0",
        "symfony/cache": "~3.0",
        "civicrm/composer-downloads-plugin": "~2.1"
    },
    "bin": ["bin/loco"],
    "autoload": {
      "psr-4": {
        "Loco\\": "src/"
      }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tim Otten",
            "email": "totten@civicrm.org"
        }
    ],
    "extra": {
        "downloads": {
            "box": {
                "url": "https://github.com/humbug/box/releases/download/3.8.3/box.phar",
                "path": "bin/box",
                "type": "phar"
            }
        }
    }
}
